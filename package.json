{
  "name": "spass",
  "displayName": "Spaß",
  "description": "Spaß sintax highlighting and code snippets",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.73.0"
  },
  "publisher": "Ghiiso",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ghiiso/spass.git"
  },
  "icon": "icons/icon.png",
  "keywords": [
    "spass","Spass"
  ],
  "author": {
    "name": "Davide Ghisolfi"
  },
  "categories": [
    "Programming Languages","Snippets","Education"
  ],
  "contributes": {
    "languages": [{
      "id": "spass",
      "aliases": ["Spass", "spass"],
      "extensions": [".sps",".spass"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "icons/icon_light.png",
        "dark": "icons/icon_dark.png"
      }
    }],
    "grammars": [{
      "language": "spass",
      "scopeName": "source.spass",
      "path": "./syntaxes/spass.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "spass",
        "path": "./snippets.json"
      }
    ]
  }
}
